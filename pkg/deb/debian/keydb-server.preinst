#!/bin/sh
set -e

case "$1" in
    install|upgrade)
        # Ensure parent directories exist before dpkg unpacks files
        mkdir -p /usr/lib/systemd/system
        mkdir -p /etc/keydb
        ;;
    abort-upgrade)
        ;;
    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0
